{
  "name": "falcon-creatives",
  "version": "1.0.0",
  "description": "Falcon Creatives - Digital Marketing & Creative Services",
  "main": "index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:analyze": "npm run build && npx serve -s build -l 3000",
    "build:production": "npm run build",
    "test": "react-scripts test",
    "test:coverage": "echo 'Tests skipped for deployment'",
    "eject": "react-scripts eject",
    "preview": "npx serve -s build -l 3000",
    "deploy:netlify": "npm run build:production && netlify deploy --prod --dir=build",
    "deploy:vercel": "npm run build:production && vercel --prod",
    "deploy:github": "npm run build:production && gh-pages -d build",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,json,css,md}",
    "format:check": "prettier --check src/**/*.{js,jsx,ts,tsx,json,css,md}",
    "type-check": "tsc --noEmit",
    "clean": "rimraf build dist",
    "postbuild": "node scripts/build-production.js",
    "generate-sitemap": "node scripts/build-production.js"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "@mui/material": "^5.15.0",
    "@mui/icons-material": "^5.15.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "framer-motion": "^10.16.16",
    "react-intersection-observer": "^9.5.3",
    "react-typed": "^2.0.12",
    "web-vitals": "^3.5.0",
    "cross-env": "^7.0.3"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-react-app": "^7.0.1",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "serve": "^14.2.1",
    "netlify-cli": "^17.10.1",
    "vercel": "^33.3.1",
    "gh-pages": "^6.1.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
